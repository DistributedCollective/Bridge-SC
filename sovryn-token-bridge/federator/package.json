{
    "name": "federator",
    "private": "true",
    "version": "1.1.0",
    "description": "Mutitoken Bridge Federator",
    "keywords": [
        "rsk",
        "federated",
        "bridge",
        "ethereum",
        "federator"
    ],
    "author": "IOVLabs",
    "scripts": {
        "start": "node ./src/main.js",
        "test": "jest --detectOpenHandles test/",
        "coverage": "jest --detectOpenHandles --coverage test/",
