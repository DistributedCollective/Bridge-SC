        "ethereumjs-tx": "^1.3.7",
        "ethereumjs-util": "^6.1.0",
        "log4js": "^5.0.0",
        "telegraf": "^4.3.0",
        "web3": "^1.6.1"
    },
    "devDependencies": {
        "eslint": "^8.9.0",
        "eslint-config-prettier": "^8.4.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^26.4.2",
        "node-notifier": "^8.0.1",
        "prettier": "^2.5.1",
        "web3-core-promievent": "^1.2.11"
    }
