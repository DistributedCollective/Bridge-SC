        "mainChain": "node integrationTest/runGanacheMainchain.js",
        "sideChain": "js integrationTest/runGanacheSidechain.js"
    },
    "dependencies": {
        "@ethereumjs/common": "^2.6.0",
        "@ethereumjs/tx": "^3.4.0",
        "ataraxia": "^0.12.0",
